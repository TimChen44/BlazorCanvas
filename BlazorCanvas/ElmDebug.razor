
<line x1="@X1" y1="@Y1" x2="@(X1+50)" y2="@(Y1+50)" style="stroke:rgb(0,255,0);stroke-width:2" />

@code {
	[Parameter]
	public int X1 { get; set; } = 60;
	[Parameter]
	public int Y1 { get; set; } = 60;

	public Guid Id { get; set; } = Guid.NewGuid();

	public Dictionary<string, object> Parameters
	{
		get
		{
			return new()
			{
				{ nameof(X1), X1 },
				{ nameof(Y1), Y1 },
			};
		}
	}
 public	RenderFragment child;
	public ElmDebug()
	{
		child = (builder) =>
					{
						builder.OpenComponent<ElmDebug>(0);
						builder.AddAttribute(1, "X1", X1);
						builder.AddAttribute(2, "Y1", Y1);
						builder.CloseComponent();
					};
	}

}
